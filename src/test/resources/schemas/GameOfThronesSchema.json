{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "url",
    "name",
    "gender",
    "culture",
    "born",
    "died",
    "titles",
    "aliases",
    "father",
    "mother",
    "spouse",
    "allegiances",
    "books",
    "povBooks",
    "tvSeries",
    "playedBy"
  ],
  "properties": {
    "url": {
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "https://anapioficeandfire.com/api/characters/583"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Jon Snow"
      ],
      "pattern": "^(.*)$"
    },
    "gender": {
      "type": "string",
      "title": "The Gender Schema",
      "default": "",
      "examples": [
        "Male"
      ],
      "pattern": "^(.*)$"
    },
    "culture": {
      "type": "string",
      "title": "The Culture Schema",
      "default": "",
      "examples": [
        "Northmen"
      ],
      "pattern": "^(.*)$"
    },
    "born": {
      "type": "string",
      "title": "The Born Schema",
      "default": "",
      "examples": [
        "In 283 AC"
      ],
      "pattern": "^(.*)$"
    },
    "died": {
      "type": "string",
      "title": "The Died Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "titles": {
      "type": "array",
      "title": "The Titles Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Lord Commander of the Night's Watch"
        ],
        "pattern": "^(.*)$"
      }
    },
    "aliases": {
      "type": "array",
      "title": "The Aliases Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Lord Snow",
          "Ned Stark's Bastard",
          "The Snow of Winterfell",
          "The Crow-Come-Over",
          "The 998th Lord Commander of the Night's Watch",
          "The Bastard of Winterfell",
          "The Black Bastard of the Wall",
          "Lord Crow"
        ],
        "pattern": "^(.*)$"
      }
    },
    "father": {
      "type": "string",
      "title": "The Father Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "mother": {
      "type": "string",
      "title": "The Mother Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "spouse": {
      "type": "string",
      "title": "The Spouse Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "allegiances": {
      "type": "array",
      "title": "The Allegiances Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "https://anapioficeandfire.com/api/houses/362"
        ],
        "pattern": "^(.*)$"
      }
    },
    "books": {
      "type": "array",
      "title": "The Books Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "https://anapioficeandfire.com/api/books/5"
        ],
        "pattern": "^(.*)$"
      }
    },
    "povBooks": {
      "type": "array",
      "title": "The Povbooks Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "https://anapioficeandfire.com/api/books/1",
          "https://anapioficeandfire.com/api/books/2",
          "https://anapioficeandfire.com/api/books/3",
          "https://anapioficeandfire.com/api/books/8"
        ],
        "pattern": "^(.*)$"
      }
    },
    "tvSeries": {
      "type": "array",
      "title": "The Tvseries Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Season 1",
          "Season 2",
          "Season 3",
          "Season 4",
          "Season 5",
          "Season 6"
        ],
        "pattern": "^(.*)$"
      }
    },
    "playedBy": {
      "type": "array",
      "title": "The Playedby Schema",
      "items": {
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Kit Harington"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}